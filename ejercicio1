#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
	int neumaticos, total,precio;
	cout << "neumaticos a comprar ";
	cin >> neumaticos;
	if (neumaticos < 3)
	{
		precio = 145;

	}
	else if ((3 <= neumaticos) || (neumaticos <= 5))
	{
		precio = 138;
	}
	else
	{
		precio = 135;
	}
	total = precio*neumaticos;

	cout << "total a pagar: " << total;

	_getch();
}
